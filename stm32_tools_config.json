{
  "description": "STM32 Development Tools Configuration",
  "version": "1.0.0",
  "tools": {
    "stm32cubeide": {
      "path": "C:/ST/STM32CubeIDE_1.18.1/STM32CubeIDE",
      "description": "STM32CubeIDE installation directory",
      "required": true,
      "executable": "stm32cubeide.exe"
    },
    "stm32edgeai": {
      "path": "C:/ST/STEdgeAI/3.0/Utilities/windows/stedgeai.exe",
      "description": "STM32 Edge AI tool (stedgeai) executable path",
      "required": true,
      "executable": "stedgeai.exe"
    },
    "stm32programmer": {
      "path": "C:/Program Files/STMicroelectronics/STM32Cube/STM32CubeProgrammer/bin/STM32_Programmer_CLI.exe",
      "description": "STM32CubeProgrammer CLI tool path",
      "required": true,
      "executable": "STM32_Programmer_CLI.exe"
    },
    "stm32signingtool": {
      "path": "C:/Program Files/STMicroelectronics/STM32Cube/STM32CubeProgrammer/bin/STM32_SigningTool_CLI.exe",
      "description": "STM32 Signing Tool CLI path",
      "required": true,
      "executable": "STM32_SigningTool_CLI.exe"
    },
    "arm_gcc_toolchain": {
      "path": "C:/ST/STM32CubeIDE_1.18.1/STM32CubeIDE/plugins/com.st.stm32cube.ide.mcu.externaltools.gnu-tools-for-stm32.13.3.rel1.win32_1.0.0.202411081344/tools/bin",
      "description": "ARM GCC toolchain bin directory",
      "required": true,
      "executable": "arm-none-eabi-gcc.exe"
    },
    "external_loader": {
      "path": "C:/Program Files/STMicroelectronics/STM32Cube/STM32CubeProgrammer/bin/ExternalLoader/MX66UW1G45G_STM32N6570-DK.stldr",
      "description": "External flash loader for STM32N6570-DK",
      "required": true
    }
  },
  "memory_layout": {
    "fsbl_address": "0x70000000",
    "application_address": "0x70100000"
  },
  "build": {
    "application_binary_path": "./embedded/STM32CubeIDE/Debug/STM32N6_FaceRecognition.bin",
    "description": "Default path to application binary for signing"
  },
  "models": {
      "face_recognition": {
      "name": "face_recognition", 
      "filename": "mobilefacenet_int8_faces.onnx",
      "address": "0x72000000",
      "target": "stm32n6",
      "input_data_type": "float32",
      "stedgeai_options": "-O0 --all-buffers-info --mvei --cache-maintenance --Oalt-sched --enable-virtual-mem-pools --Omax-ca-pipe 4 --Ocache-opt --Os --enable-epoch-controller"
    },
    "face_detection": {
      "name": "face_detection",
      "filename": "centerface.tflite",
      "address": "0x71000000",
      "target": "stm32n6",
      "input_data_type": "float32",
      "stedgeai_options": "-O0 --all-buffers-info --mvei --cache-maintenance --Oalt-sched --enable-virtual-mem-pools --Omax-ca-pipe 4 --Ocache-opt --Os --enable-epoch-controller"
    }

  }
}
